{
  "name": "@macknolandev/marianatek-node",
  "description": "A Node.js client for the Mariana Tek API",
  "version": "0.0.7",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/mackenzienolan/marianatek-node.git"
  },
  "keywords": [
    "Mariana Tek",
    "MarianaTek",
    "marianatek",
    "marianatek-node",
    "marianatek-api",
    "marianatek-sdk"
  ],
  "exports": {
    "types": "./distribution/index.d.ts",
    "default": "./distribution/index.js"
  },
  "files": [
    "distribution"
  ],
  "main": "./distribution/index.js",
  "types": "./distribution/index.d.ts",
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "author": {
    "name": "Mackenzie Nolan",
    "email": "mack@macknolan.com",
    "url": "https://macknolan.dev"
  },
  "scripts": {
    "build": "del-cli distribution && tsc --project tsconfig.dist.json",
    "clean": "rm -rf dist",
    "dev": "tsup src/index.ts --format cjs,esm --watch --dts",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "test": "vitest --run --passWithNoTests",
    "release": "np"
  },
  "packageManager": "pnpm@9.12.2",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "del-cli": "^6.0.0",
    "np": "^10.2.0",
    "tsup": "^8.3.6",
    "vitest": "^3.0.5"
  },
  "dependencies": {
    "ky": "^1.7.4",
    "zod": "^3.24.1"
  }
}
